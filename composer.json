{
    "name": "camelot/api-importer",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-pcntl": "*",
        "league/csv": "^9.9"
    },
    "require-dev": {
        "camelot/coding-style": "^3.0",
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.19 || ^3.0",
        "friendsofphp/php-cs-fixer": "^3.51",
        "justinrainbow/json-schema": "^5.2",
        "phpunit/phpunit": "^10.5",
        "symfony/console": "^6.3 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\ApiImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\ApiImporter\\Tests\\": "tests/"
        }
    },
    "scripts": {
    }
}
